// template 3

class Solution {
    public int findMin(int[] nums) {
        if(nums == null || nums.length == 0)
            throw new IllegalArgumentException("no slution");
        int start = 0, end = nums.length-1;
        
        while(start <= end) {
            int mid = (end - start)/2 + start;
            if(nums[mid] < nums[end]) {
                end = mid;
            } else if(nums[mid] > nums[end]){
                start = mid + 1;
            } else {
                end--;
            }
        }
        return nums[start];
    }
}


//O(logn), O(1)

